{
  "appId": "relicblade.deckbuilder",
  "appName": "Relicblade Deck Builder",
  "bundledWebRuntime": false,
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "SplashScreen": {
      "launchShowDuration": 2000
    },
    "Keyboard": {
      "resize": "none",
      "style": "DARK",
      "resizeOnFullScreen": false
    }
  }
}